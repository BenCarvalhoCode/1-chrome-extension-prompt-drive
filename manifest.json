{
  "manifest_version": 3,
  "name": "Prompt DRIVE",
  "version": "1.0.0",
  "description": "Gerenciador de prompts",
  "action": {
    "default_popup": "index.html",
    "default_title": "Prompt DRIVE"
  },
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  }
}
